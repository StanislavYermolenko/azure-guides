{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "clusterName": {
            "value": "<Insert Cluster Name>"
        },
        "dnsPrefix": {
            "value": "<Insert DNS Name>"
        },
        "agentCount": {
            "value": 3
        },
        "k8sversion": {
            "value": "1.15.10"
        },
        "agentVMSize":{
            "value": "Standard_DS2_v2"
        },
        "linuxAdminUsername": {
            "value": "aksadmin"
        },
        "sshRSAPublicKey": {
            "value": "<Insert SSH Public Key>"
        },
        "servicePrincipalClientId": {
            "value": "<Insert Service Principal App ID>"
        },
        "servicePrincipalClientSecret": {
            "value": "<Insert Service Principal Password>"
        }
    }
}
